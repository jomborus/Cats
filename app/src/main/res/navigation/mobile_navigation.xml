<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="kotu.cats.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="kotu.cats.ui.dashboard.BreedsFragment"
        android:label="@string/title_breeds"
        tools:layout="@layout/fragment_breeds" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="kotu.cats.ui.notifications.ImageFragment"
        android:label="@string/title_images"
        tools:layout="@layout/fragment_image" />
